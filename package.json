{
  "name": "slidev-theme-andyjjrt",
  "version": "0.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.43.7",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.7.3"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.7",
    "@slidev/cli": "^0.43.7"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Outfit, Noto Sans TC",
        "mono": "Ubuntu Mono"
      },
      "hightlighter": "prism"
    }
  },
  "description": "[![NPM version](https://img.shields.io/npm/v/slidev-theme-andyjjrt?color=3AB9D4&label=)](https://www.npmjs.com/package/slidev-theme-andyjjrt)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyjjrt/slidev-theme-andyjjrt.git"
  },
